{
  "family": "auth-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "auth-service",
      "image": "211125300512.dkr.ecr.us-east-1.amazonaws.com/authentication-service:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4001,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "MONGO_URI",
          "value": "PLACEHOLDER_MONGO_URI"
        }
      ]
    }
  ]
}
